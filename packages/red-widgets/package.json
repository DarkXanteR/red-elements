{
  "name": "@tecla5/red-widgets",
  "productName": "red-widgets",
  "private": true,
  "description": "node-red jQuery widgets, controller logic and styles",
  "version": "0.0.1",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "test": "jest --no-cache --config ./jest.config.js",
    "test:watch": "jest --watch",
    "lerna:bootstrap": "lerna bootstrap --scope @tecla5/red-widgets",
    "lerna:update": "npm run lerna:clean && npm run lerna:bootstrap",
    "lerna:update:win": "npm run lerna:clean:win && npm run lerna:bootstrap",
    "lerna:clean": "rm -f package.json.lock 2> /dev/null && echo y | lerna clean --scope @tecla5/red-widgets",
    "lerna:clean:win": "npm run clean:lock | lerna clean --scope @tecla5/red-widgets",
    "clean:win": "npm run clean:lock | clean:modules",
    "clean:lock": "del /s /f /q package-lock.json.lock",
    "clean:modules": "del /s /f /q node_modules/*",
    "link:shared": "npm link @tecla5/red-widgets"
  },
  "dependencies": {
    "@tecla5/red-shared": "^0.0.1",
    "ace-builds": "^1.2.8",
    "bootstrap": "^3.3.7",
    "bootstrap-select": "^1.12.4",
    "brace": "^0.11.0",
    "d3": "^4.10.2",
    "font-awesome": "^4.7.0",
    "i18next": "^9.0.1",
    "jquery": "^3.2.1",
    "jquery-ui-dist": "^1.12.1",
    "jsonata": "^1.3.1",
    "marked": "^0.3.6",
    "normalize.css": "^7.0.0",
    "promise-polyfill": "^6.0.2",
    "reflect-metadata": "^0.1.10",
    "babel-runtime": "^6.26.0"
  },
  "devDependencies": {
    "inversify": "^4.5.2",
    "inversify-inject-decorators": "^3.0.2",
    "@types/jquery": "^3.2.16",
    "@types/d3": "^4.12.0",
    "@types/jqueryui": "^1.11.37",
    "@types/jest": "^21.1.9",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^21.2.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "browser-env": "^2.0.31",
    "cross-env": "^5.0.0",
    "enzyme": "^3.1.1",
    "enzyme-to-json": "^3.2.2",
    "faker": "^4.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "jest-jquery-matchers": "^1.4.2",
    "promise": "^8.0.1",
    "puppeteer": "^0.13.0",
    "ts-jest": "^21.2.4",
    "typescript": "2.6.2"
  }
}
